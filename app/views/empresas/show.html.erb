<div class="row-fluid">
  <div class="span4 well">

    <p>
      <strong>Nome:</strong>
      <%= @empresa.nome %>
    </p>

    <p>
      <strong>CNPJ:</strong>
      <%= @empresa.cnpj %>
    </p>

    <p>
      <strong>Email:</strong>
      <%= @empresa.email %>
    </p>

    <p>
      <strong>Telefone:</strong>
      <%= @empresa.telefone %>
    </p>

    <%= link_to 'Editar', edit_empresa_path(@empresa) %> |
    <%= link_to 'Voltar', empresas_path %>
  </div>
</div>
