<div class="row-fluid">
  <div class="span4 well">
    <p id="notice"><%= notice %></p>

    <p>
      <strong>Nome:</strong>
      <%= @cliente.nome %>
    </p>

    <p>
      <strong>Cpf:</strong>
      <%= @cliente.cpf %>
    </p>

    <p>
      <strong>Email:</strong>
      <%= @cliente.email %>
    </p>

    <p>
      <strong>Telefone:</strong>
      <%= @cliente.telefone %>
    </p>

    <p>
      <strong>Celular:</strong>
      <%= @cliente.celular %>
    </p>

    <p>
      <strong>Nascimento:</strong>
      <%= @cliente.nascimento %>
    </p>

    <%= link_to 'Edit', edit_cliente_path(@cliente) %> |
    <%= link_to 'Back', clientes_path %>
  </div>
</div>
